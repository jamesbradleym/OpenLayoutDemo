{
  "$schema": "https://hypar.io/Schemas/Function.json",
  "id": "4d0fc267-9560-42b4-a9be-d075e0bfc4e3",
  "name": "Open Layout - Demo",
  "description": "The Open Layout Demo function.",
  "language": "C#",
  "model_output": "Object Layout",
  "model_dependencies": [
    {
      "autohide": false,
      "name": "Space Planning Zones",
      "optional": true
    },
    {
      "autohide": false,
      "name": "Foo",
      "optional": false
    }
  ],
  "input_schema": {
    "type": "object",
    "properties": {
      "Extract": {
        "type": "array",
        "items": {
          "type": "object",
          "$hyparStyle": "row",
          "properties": {
            "Category": {
              "description": "Select the Rhino model category to work with",
              "type": [
                "string",
                "null"
              ],
              "$hyparOrder": 0,
              "$hyparEnumQuery": {
                "query": "[*discriminator=Elements.MeshElement].Category",
                "dependency": "Foo"
              }
            },
            "Program": {
              "type": [
                "string",
                "null"
              ],
              "$hyparOrder": 1,
              "$hyparEnumQuery": {
                "query": "[*discriminator=Elements.SpaceBoundary].Program Type",
                "dependency": "Space Planning Zones"
              }
            },
            "Layout": {
              "description": "What layout strategy should be used?",
              "type": "string",
              "$hyparAutocomplete": [
                "Row",
                "Cell"
              ],
              "$hyparOrder": 2
            },
            "Aisle": {
              "type": "number",
              "default": 2.0,
              "minimum": 0.0,
              "$hyparShowIf": {
                "conditions": [
                  {
                    "property": "Layout",
                    "value": "Row"
                  }
                ]
              },
              "$hyparOrder": 3
            },
            "Gap": {
              "type": "number",
              "default": 0.001,
              "minimum": 0.001,
              "$hyparShowIf": {
                "conditions": [
                  {
                    "property": "Layout",
                    "value": "Row"
                  }
                ]
              },
              "$hyparOrder": 4
            },
            "Forward": {
              "type": "number",
              "default": 0.0,
              "minimum": 0.0,
              "$hyparShowIf": {
                "conditions": [
                  {
                    "property": "Layout",
                    "value": "Row"
                  }
                ]
              },
              "$hyparOrder": 5
            },
            "Backward": {
              "type": "number",
              "default": 0.0,
              "minimum": 0.0,
              "$hyparShowIf": {
                "conditions": [
                  {
                    "property": "Layout",
                    "value": "Row"
                  }
                ]
              },
              "$hyparOrder": 6
            },
            "Show Pattern": {
              "type": "boolean",
              "default": false,
              "$hyparShowIf": {
                "conditions": [
                  {
                    "property": "Layout",
                    "value": "Row"
                  }
                ]
              },
              "$hyparOrder": 20
            }
          }
        }
      }
    }
  },
  "outputs": [
    {
      "name": "Instances",
      "description": "The number of placed instances.",
      "type": "number"
    }
  ],
  "element_types": [
    "https://prod-api.hypar.io/schemas/SpaceBoundary"
  ],
  "repository_url": "https://github.com/hypar-io/function",
  "last_updated": "0001-01-01T00:00:00",
  "cli_version": "1.6.0"
}